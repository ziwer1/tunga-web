{
    "name": "tunga-web",
    "scripts": {
        "start": "node ./scripts/server.js",
        "server": "nodemon ./scripts/server.js",
        "build:local": "cross-env BACKEND_ROOT=http://localhost:8000/ webpack --progress --profile --colors",
        "build": "cross-env NODE_ENV=production BUILD_DEV=0 BUILD_PRERELEASE=0 webpack -p --config webpack.production.config.js --progress --profile --colors --optimize-minimize --optimize-dedupe",
        "build:sandbox": "cross-env NODE_ENV=production STRIPE_ENV=development ANALYTICS_ENV=development BUILD_DEV=0 BUILD_PRERELEASE=1 webpack -p --config webpack.production.config.js --progress --profile --colors --optimize-minimize --optimize-dedupe",
        "build:sandbox:stripelive": "cross-env NODE_ENV=production ANALYTICS_ENV=development BUILD_DEV=0 BUILD_PRERELEASE=1 webpack -p --config webpack.production.config.js --progress --profile --colors --optimize-minimize --optimize-dedupe",
        "watch:local": "cross-env BUILD_DEV=1 BACKEND_ROOT=http://localhost:8000/ webpack-dev-server --hot --inline --progress --colors",
        "watch:local:prod": "cross-env NODE_ENV=production ANALYTICS_ENV=development BUILD_DEV=0 BACKEND_ROOT=http://localhost:8000/ webpack-dev-server --hot --inline --progress --colors",
        "watch": "cross-env BUILD_DEV=1 webpack-dev-server --hot --inline --progress --colors",
        "lint": "node ./scripts/tasks/eslint.js",
        "prettier": "node ./scripts/prettier/index.js write-changed",
        "prettier-all": "node ./scripts/prettier/index.js write",
        "deploy": "cd .ansible && ansible-playbook deploy.yml -i env/prod && cd ..",
        "deploy:sandbox": "cd .ansible && ansible-playbook deploy.yml -i env/sandbox && cd ..",
        "test": "echo \"No unit tests and e2e tests yet\" && exit 0"
    },
    "version": "0.1.0",
    "license": "MIT",
    "dependencies": {
        "Dante2": "^0.4.3",
        "animate.css": "^3.5.2",
        "axios": "^0.15.3",
        "babel-runtime": "^6.9.2",
        "btoa": "^1.1.2",
        "draft-convert": "^2.0.1",
        "draft-js": "^0.10.5",
        "es6-promise": "^4.0.5",
        "history": "^4.5.0",
        "js-cookie": "^2.1.3",
        "lodash": "^4.13.1",
        "markdown": "^0.5.0",
